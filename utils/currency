#!/usr/bin/env/perl

use strict;
use warnings;
use feature qw(say);

use Data::Dumper;

use Mojo::DOM;
use Mojo::Collection qw(c);
use List::Util       qw(uniq);

my $dom = Mojo::DOM
  ->new(join '', <DATA>)
  ->find('tbody')
  ->sort(
     sub {
        $a->child_nodes->grep(sub { $_->type eq 'tag' })->each <=> $b->child_nodes->grep(sub { $_->type eq 'tag' })->each;
     }
  )
  ->last
  ->find('tr')
  ->grep(sub { $_->find('th')->each ? 0 : 1 });

my $currencies;
foreach my $tr ($dom->each) {
   my @tds = $tr->find('td')->each;

   push @$currencies, map {
      [map { s/^\s+//r =~ s/\s+$//r =~ s/\s{2,}|\R/ /r } @$_]
   } map {
      [map { $_->all_text } @$_]
   } @tds == 5 ? [$tds[1], $tds[2]] : [$tds[2], $tds[3]];
}

$currencies = [c(@$currencies)->uniq(sub { $_->[1] eq '(none)' ? $_->[0] : $_->[1] })->each];
my %sym;
my @codes;
foreach my $v (@$currencies) {
   next if $v->[0] eq '(none)';
   next if $v->[1] eq '(none)';

   my ($k, $k1) = $v->[0] =~ m/^\s*(\S+) or (\S+).+$/g;
   $k //= $v->[0] and push $sym{$k}->@*, $v->[1];
   push $sym{$k1}->@*, $v->[1] if defined $k1;
   push @codes, $v->[1];

   #$v->[0] = $v->[0] eq '(none)' ? $v->[1] : $v->[0];
   #say "$v->[1] => $v->[0]";
}

$sym{$_} = [uniq $sym{$_}->@*] foreach keys %sym;
$Data::Dumper::Useqq     = 0;
$Data::Dumper::Quotekeys = 0;
say Data::Dumper->Dump([{%sym}, [@codes]], [qw(SYMBOLS *CODES)]);

__DATA__
